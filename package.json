{
    "name": "AlgoInt",
    "version": "0.0.1",
    "description": "AlgoInt is a web application made for practicing coding interviews.",
    "main": "/src/app.jsx",
    "scripts": {
        "start": "npm run clean && webpack-dev-server --config ./build-config/webpack.dev.config.js",
        "build": "npm run clean && webpack --config ./build-config/webpack.config.js",
        "deploy": "npm run build && gh-pages -d build",
        "clean": "rm -Rf ./build/bundle.js ./build/index.html",
        "reset": "npm run clean && rm -Rf ./node_modules ./package-lock.json"
    },
    "keywords": [],
    "author": "",
    "license": "MIT",
    "dependencies": {
        "@material-ui/core": "^4.9.7",
        "ace-builds": "^1.4.8",
        "bootstrap": "^4.4.1",
        "core-js": "^3.1.4",
        "firebase": "^7.11.0",
        "react": "^16.8.6",
        "react-ace": "^8.1.0",
        "react-bootstrap": "^1.0.0-beta.17",
        "react-countdown": "^2.2.1",
        "react-dom": "^16.8.6",
        "react-icons": "^3.9.0",
        "react-modal": "^3.11.2",
        "react-redux": "^7.1.0",
        "react-router-dom": "^5.1.2",
        "redux": "^4.0.2",
        "redux-thunk": "^2.3.0"
    },
    "devDependencies": {
        "@babel/core": "^7.5.0",
        "@babel/plugin-proposal-class-properties": "^7.8.3",
        "@babel/preset-env": "^7.5.0",
        "@babel/preset-react": "^7.0.0",
        "babel-loader": "^8.0.6",
        "css-loader": "^3.0.0",
        "gh-pages": "^2.0.1",
        "html-loader": "^0.5.5",
        "html-webpack-harddisk-plugin": "^1.0.1",
        "html-webpack-plugin": "^3.2.0",
        "node-sass": "^7.0.0",
        "redux-devtools-extension": "^2.13.8",
        "sass-loader": "^7.1.0",
        "style-loader": "^0.23.1",
        "webpack": "^4.35.2",
        "webpack-cli": "^3.3.5",
        "webpack-dev-server": "^3.7.2"
    },
    "proxy": {
        "/api": {
            "target": "http://localhost:5000/"
        },
        "/sync": {
            "target": "ws://localhost:5000/",
            "ws": true
        }
    }
}
